<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".chatbot">

    <ImageView
            android:layout_width="746dp"
            android:layout_height="937dp" app:srcCompat="@drawable/background_small" android:id="@+id/imageView6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.626"
            app:layout_constraintVertical_bias="0.956"/>
    <EditText
            android:hint="Escribe tus ingredientes y veamos que podrias cocinar con ellos"
            android:layout_width="389dp"
            android:layout_height="79dp" android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.68" app:layout_constraintVertical_bias="0.98"/>
    <TextView
            android:text="TextView"
            android:layout_width="350dp"
            android:layout_height="39dp" android:id="@+id/textView2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.491" app:layout_constraintVertical_bias="0.113"/>
    <TextView
            android:text="TextView"
            android:layout_width="326dp"
            android:layout_height="236dp" android:id="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.529" app:layout_constraintVertical_bias="0.254"/>
    <Button
            android:id="@+id/speakerButton"
            android:backgroundTint="@android:color/transparent"
            android:layout_width="87dp"
            android:layout_height="71dp"
            android:background="@null"
            android:src="@android:drawable/ic_lock_silent_mode_off"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.978"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.018" tools:ignore="SpeakableTextPresentCheck"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_lock_silent_mode_off"
            android:id="@+id/imageView9"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.918" app:layout_constraintVertical_bias="0.046"/>
</androidx.constraintlayout.widget.ConstraintLayout>